PREFIX fhir: <http://hl7.org/fhir/> 
PREFIX fhirvs: <http://hl7.org/fhir/ValueSet/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 


#imported_begin 
IMPORT <uri.shex>
#imported_end
#imported_begin 
IMPORT <Group.shex>
#imported_end
#imported_begin 
IMPORT <Person.shex>
#imported_end
#imported_begin 
IMPORT <Device.shex>
#imported_end
#imported_begin 
IMPORT <Extension.shex>
#imported_end
#imported_begin 
IMPORT <Reference.shex>
#imported_end
#imported_begin 
IMPORT <Organization.shex>
#imported_end
# Who or what the contact is
<cqf-contactReference> EXTENDS @<Extension> CLOSED {   

    a [fhir:cqf-contactReference]?;
    fhir:nodeRole [fhir:treeRoot]?;

    fhir:url @<uri> AND
    	{fhir:value ["http://hl7.org/fhir/StructureDefinition/cqf-contactReference"]}; 
    fhir:value (@<Reference>  AND {fhir:link @<Person> OR 
    			 @<Group> OR 
    			 @<Device> OR 
    			 @<Organization> ?})  ; 
    ^fhir:extension { a [fhir:ContactDetail]}

}  

