PREFIX fhir: <http://hl7.org/fhir/> 
PREFIX fhirvs: <http://hl7.org/fhir/ValueSet/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 


#imported_begin 
IMPORT <uri.shex>
#imported_end
#imported_begin 
IMPORT <Extension.shex>
#imported_end
#imported_begin 
IMPORT <Reference.shex>
#imported_end
#imported_begin 
IMPORT <DiagnosticReport.shex>
#imported_end
# Replacement for another report
<diagnosticReport-replaces> EXTENDS @<Extension> CLOSED {   

    a [fhir:diagnosticReport-replaces]?;
    fhir:nodeRole [fhir:treeRoot]?;

    fhir:url @<uri> AND
    	{fhir:value ["http://hl7.org/fhir/StructureDefinition/diagnosticReport-replaces"]}; 
    fhir:value (@<Reference>  AND {fhir:link @<DiagnosticReport> ?})  ; 
    ^fhir:extension { a [fhir:DiagnosticReport]}

}  

