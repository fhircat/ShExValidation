PREFIX fhir: <http://hl7.org/fhir/> 
PREFIX fhirvs: <http://hl7.org/fhir/ValueSet/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 


#imported_begin 
IMPORT <uri.shex>
#imported_end
#imported_begin 
IMPORT <Resource.shex>
#imported_end
#imported_begin 
IMPORT <Extension.shex>
#imported_end
#imported_begin 
IMPORT <Reference.shex>
#imported_end
# Request(s) replaced by this request
<request-replaces> EXTENDS @<Extension> CLOSED {   

    a [fhir:request-replaces]?;
    fhir:nodeRole [fhir:treeRoot]?;

    fhir:url @<uri> AND
    	{fhir:value ["http://hl7.org/fhir/StructureDefinition/request-replaces"]}; 
    fhir:value (@<Reference>  AND {fhir:link @<Resource> ?})  ; 
    ^fhir:extension { a [fhir:NutritionOrder]} OR 
          {a [fhir:SupplyRequest]} OR 
          {a [fhir:Task]}

}  

