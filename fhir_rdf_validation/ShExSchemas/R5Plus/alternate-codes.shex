PREFIX fhir: <http://hl7.org/fhir/> 
PREFIX fhirvs: <http://hl7.org/fhir/ValueSet/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 


#imported_begin 
IMPORT <uri.shex>
#imported_end
#imported_begin 
IMPORT <Extension.shex>
#imported_end
#imported_begin 
IMPORT <CodeableConcept.shex>
#imported_end
# Alternate code/original text for this code
<alternate-codes> EXTENDS @<Extension> CLOSED {   

    a [fhir:alternate-codes]?;
    fhir:nodeRole [fhir:treeRoot]?;

    fhir:url @<uri> AND
    	{fhir:value ["http://hl7.org/fhir/StructureDefinition/coding-purpose"]}; 
    fhir:value @<CodeableConcept>  ; 
    ^fhir:extension { a [fhir:code]}

}  

