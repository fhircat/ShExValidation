PREFIX fhir: <http://hl7.org/fhir/> 
PREFIX fhirvs: <http://hl7.org/fhir/ValueSet/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 


#imported_begin 
IMPORT <uri.shex>
#imported_end
#imported_begin 
IMPORT <aux.shex>
#imported_end
#imported_begin 
IMPORT <Extension.shex>
#imported_end
# A context-specific sex parameter for clinical use
<patient-sexParameterForClinicalUse> EXTENDS @<Extension> CLOSED {   

    a [fhir:patient-sexParameterForClinicalUse]?;
    fhir:nodeRole [fhir:treeRoot]?;

    fhir:value @<Extension>; 
    fhir:period @<Extension>?; 
    fhir:comment @<Extension>?; 
    fhir:supportingInfo @<OneOrMore_Extension>?; 
    fhir:url @<uri> AND
    	{fhir:value ["http://hl7.org/fhir/StructureDefinition/patient-sexParameterForClinicalUse"]}; 
    ^fhir:extension { a [fhir:Resource]}

}  

