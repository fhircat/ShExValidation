PREFIX fhir: <http://hl7.org/fhir/> 
PREFIX fhirvs: <http://hl7.org/fhir/ValueSet/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 


#imported_begin 
IMPORT <uri.shex>
#imported_end
#imported_begin 
IMPORT <date.shex>
#imported_end
#imported_begin 
IMPORT <Extension.shex>
#imported_end
# Short description - single paragraph, plain string
<metadataresource-publish-date> EXTENDS @<Extension> CLOSED {   

    a [fhir:metadataresource-publish-date]?;
    fhir:nodeRole [fhir:treeRoot]?;

    fhir:url @<uri> AND
    	{fhir:value ["http://hl7.org/fhir/StructureDefinition/metadataresource-publish-date"]}; 
    fhir:value @<date>  ; 
    ^fhir:extension { a [fhir:MetadataResource]}

}  

