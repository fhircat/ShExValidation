PREFIX fhir: <http://hl7.org/fhir/> 
PREFIX fhirvs: <http://hl7.org/fhir/ValueSet/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 


#imported_begin 
IMPORT <uri.shex>
#imported_end
#imported_begin 
IMPORT <Person.shex>
#imported_end
#imported_begin 
IMPORT <Patient.shex>
#imported_end
#imported_begin 
IMPORT <Extension.shex>
#imported_end
#imported_begin 
IMPORT <Reference.shex>
#imported_end
#imported_begin 
IMPORT <Practitioner.shex>
#imported_end
#imported_begin 
IMPORT <RelatedPerson.shex>
#imported_end
# Extension
<cqf-initiatingPerson> EXTENDS @<Extension> CLOSED {   

    a [fhir:cqf-initiatingPerson]?;
    fhir:nodeRole [fhir:treeRoot]?;

    fhir:url @<uri> AND
    	{fhir:value ["http://hl7.org/fhir/StructureDefinition/cqf-initiatingPerson"]}; 
    fhir:value (@<Reference>  AND {fhir:link @<Person> OR 
    			 @<Patient> OR 
    			 @<Practitioner> OR 
    			 @<RelatedPerson> ?})  ; 
    ^fhir:extension { a [fhir:Basic]}

}  

