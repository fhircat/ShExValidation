PREFIX fhir: <http://hl7.org/fhir/> 
PREFIX fhirvs: <http://hl7.org/fhir/ValueSet/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 

IMPORT <uri.shex>
IMPORT <code.shex>
IMPORT <Extension.shex>

# Allowed resource for reference
<questionnaire-referenceResource> EXTENDS @<Extension> CLOSED {   

    a [fhir:questionnaire-referenceResource];
    fhir:nodeRole [fhir:treeRoot]?;
    fhir:v @<uri> AND
    	{fhir:v ["http://hl7.org/fhir/StructureDefinition/questionnaire-referenceResource"]}; 
    fhir:v (@<code> AND
    	{fhir:v @fhirvs:resource-types} )  AND { rdf:type IRI } ; 
    ^fhir:extension { ^fhir:item {a [fhir:Questionnaire]}
        }
}  

