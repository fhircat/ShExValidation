PREFIX fhir: <http://hl7.org/fhir/> 
PREFIX fhirvs: <http://hl7.org/fhir/ValueSet/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 


#imported_begin 
IMPORT <uri.shex>
#imported_end
#imported_begin 
IMPORT <Patient.shex>
#imported_end
#imported_begin 
IMPORT <Extension.shex>
#imported_end
#imported_begin 
IMPORT <Reference.shex>
#imported_end
# Source Patient Information
<documentreference-sourcepatient> EXTENDS @<Extension> CLOSED {   

    a [fhir:documentreference-sourcepatient]?;
    fhir:nodeRole [fhir:treeRoot]?;

    fhir:url @<uri> AND
    	{fhir:value ["http://hl7.org/fhir/StructureDefinition/documentreference-sourcepatient"]}; 
    fhir:value (@<Reference>  AND {fhir:link @<Patient> ?})  ; 
    ^fhir:extension { a [fhir:DocumentReference]}

}  

