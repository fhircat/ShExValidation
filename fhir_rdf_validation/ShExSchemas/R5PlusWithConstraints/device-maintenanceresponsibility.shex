PREFIX fhir: <http://hl7.org/fhir/> 
PREFIX fhirvs: <http://hl7.org/fhir/ValueSet/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 


#imported_begin 
IMPORT <uri.shex>
#imported_end
#imported_begin 
IMPORT <Extension.shex>
#imported_end
# Device Maintenance Responsibility
<device-maintenanceresponsibility> EXTENDS @<Extension> CLOSED {   

    a [fhir:device-maintenanceresponsibility]?;
    fhir:nodeRole [fhir:treeRoot]?;

    fhir:person @<Extension>?; 
    fhir:organization @<Extension>?; 
    fhir:url @<uri> AND
    	{fhir:value ["http://hl7.org/fhir/StructureDefinition/device-maintenanceresponsibility"]}; 
    ^fhir:extension { a [fhir:Device]}

}  

