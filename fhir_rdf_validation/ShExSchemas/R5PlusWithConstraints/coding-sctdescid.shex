PREFIX fhir: <http://hl7.org/fhir/> 
PREFIX fhirvs: <http://hl7.org/fhir/ValueSet/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 


#imported_begin 
IMPORT <id.shex>
#imported_end
#imported_begin 
IMPORT <uri.shex>
#imported_end
#imported_begin 
IMPORT <Extension.shex>
#imported_end
# SNOMED CT Description ID
<coding-sctdescid> EXTENDS @<Extension> CLOSED {   

    a [fhir:coding-sctdescid]?;
    fhir:nodeRole [fhir:treeRoot]?;

    fhir:url @<uri> AND
    	{fhir:value ["http://hl7.org/fhir/StructureDefinition/coding-sctdescid"]}; 
    fhir:value @<id>  ; 
    ^fhir:extension { a [fhir:Coding]} OR 
          {^fhir:designation {^fhir:concept {a [fhir:CodeSystem]}}} OR 
          {^fhir:designation {^fhir:contains {^fhir:expansion {a [fhir:ValueSet]}}}}

}  

