PREFIX fhir: <http://hl7.org/fhir/> 
PREFIX fhirvs: <http://hl7.org/fhir/ValueSet/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 


#imported_begin 
IMPORT <uri.shex>
#imported_end
#imported_begin 
IMPORT <Resource.shex>
#imported_end
#imported_begin 
IMPORT <Extension.shex>
#imported_end
#imported_begin 
IMPORT <CodeableReference.shex>
#imported_end
# Any obstacle that limits or prevents obtaining care
<workflow-barrier> EXTENDS @<Extension> CLOSED {   

    a [fhir:workflow-barrier]?;
    fhir:nodeRole [fhir:treeRoot]?;

    fhir:url @<uri> AND
    	{fhir:value ["http://hl7.org/fhir/StructureDefinition/workflow-barrier"]}; 
    fhir:value (@<CodeableReference>  AND {fhir:link @<Resource> ?})  ; 
    ^fhir:extension { a [fhir:Goal]} OR 
          {a [fhir:CommunicationRequest]} OR 
          {^fhir:activity {a [fhir:CarePlan]}}

}  

