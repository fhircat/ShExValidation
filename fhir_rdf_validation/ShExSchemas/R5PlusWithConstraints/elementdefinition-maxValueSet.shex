PREFIX fhir: <http://hl7.org/fhir/> 
PREFIX fhirvs: <http://hl7.org/fhir/ValueSet/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 


#imported_begin 
IMPORT <uri.shex>
#imported_end
#imported_begin 
IMPORT <ValueSet.shex>
#imported_end
#imported_begin 
IMPORT <Extension.shex>
#imported_end
#imported_begin 
IMPORT <canonical.shex>
#imported_end
# Maximum Value Set (when strength = extensible)
<elementdefinition-maxValueSet> EXTENDS @<Extension> CLOSED {   

    a [fhir:elementdefinition-maxValueSet]?;
    fhir:nodeRole [fhir:treeRoot]?;

    fhir:url @<uri> AND
    	{fhir:value ["http://hl7.org/fhir/StructureDefinition/elementdefinition-maxValueSet"]}; 
    fhir:value @<uri>  OR 
    			(@<canonical>  AND {fhir:link @<ValueSet> })  ; 
    ^fhir:extension { ^fhir:binding {a [fhir:ElementDefinition]}}

}  

