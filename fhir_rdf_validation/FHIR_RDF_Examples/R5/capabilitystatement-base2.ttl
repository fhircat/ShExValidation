     fhir:status [ fhir:v "extensions" ]
  ] ;
  fhir:url [ fhir:v "http://hl7.org/fhir/CapabilityStatement/base2"], [ fhir:v "http://hl7.org/fhir/CapabilityStatement/base2"] ;
  fhir:version [ fhir:v "5.0.0"], [ fhir:v "5.0.0"] ;
