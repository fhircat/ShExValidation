@prefix fhir: <http://hl7.org/fhir/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# - resource -------------------------------------------------------------------

[a fhir:Patient ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "newborn"] ; #   
#simple newborn example
#  
  fhir:text [
     fhir:status [ fhir:v "generated" ] ;
     fhir:div "<div xmlns=\"http://www.w3.org/1999/xhtml\"><p style=\"border: 1px #661aff solid; background-color: #e6e6ff; padding: 10px;\"><b>Anonymous Patient</b> male, DoB: 2017-09-05</p><hr/><table class=\"grid\"><tr><td style=\"background-color: #f3f5da\" title=\"Known multipleBirth status of Patient\">Multiple Birth:</td><td colspan=\"3\">2</td></tr><tr><td style=\"background-color: #f3f5da\" title=\"Mother's maiden (unmarried) name, commonly collected to help verify patient identity.\"><a href=\"https://build.fhir.org/ig/HL7/fhir-extensions/StructureDefinition-patient-mothersMaidenName.html\">Patient Mother's Maiden Name:</a></td><td colspan=\"3\"><ul><li>Everywoman</li></ul></td></tr></table></div>"
  ] ; #    The details of the child   
  fhir:extension ( [
     fhir:url [ fhir:v "http://hl7.org/fhir/StructureDefinition/patient-mothersMaidenName"^^xsd:anyURI ] ;
     fhir:value [ fhir:v "Everywoman" ]
  ] ) ; # 
  fhir:gender [ fhir:v "male"] ; # 
  fhir:birthDate [
     fhir:v "2017-09-05"^^xsd:date ;
     fhir:extension ( [
       fhir:url [ fhir:v "http://hl7.org/fhir/StructureDefinition/patient-birthTime"^^xsd:anyURI ] ;
       fhir:value [ fhir:v "2017-05-09T17:11:00+01:00"^^xsd:dateTime ]
     ] )
  ] ; # 
  fhir:multipleBirth [ fhir:v "2"^^xsd:integer]] . # 

